        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_add"
        android:supportsRtl="true"
        android:theme="@style/Theme.Assurance"
        tools:targetApi="31">
        <activity
            android:name=".my_row"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".activity_update"
